{
  "name": "auctioneer-react-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "serve:test": "( [ -d \"./build\" ] || snowpack build ) && npx servor ./build/",
    "serve:build": "snowpack build && npx servor ./build/"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "history": "^5.0.0",
    "immer": "^7.0.14",
    "jwt-decode": "^3.1.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^6.11.5",
    "react-query": "^2.26.2",
    "react-router-dom": "^6.0.0-beta.0",
    "zustand": "^3.2.0"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.0.4",
    "@snowpack/plugin-react-refresh": "^2.3.7",
    "@snowpack/plugin-sass": "^1.1.1",
    "@snowpack/plugin-typescript": "^1.1.0",
    "@snowpack/web-test-runner-plugin": "^0.1.4",
    "@testing-library/react": "^11.0.0",
    "@types/chai": "^4.2.13",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/snowpack-env": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "@web/test-runner": "^0.9.7",
    "babel-eslint": "^10.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "prettier": "^2.0.5",
    "react-query-devtools": "^2.6.3",
    "snowpack": "^2.17.1",
    "typescript": "^4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
